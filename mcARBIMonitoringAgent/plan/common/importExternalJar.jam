PREFIXES:
	PREFIX isro: <http://www.arbi.com//BuiltInPlan#>;


PLAN PERFORM isro:importExternalJar($path, $classFullName) {
	ID: "http://www.arbi.com//importExternalJar"
	PRECONDITION :
		FACT JarLoader($loader); 
		FACT WorldModelHandler($handler);
	BODY:
		PERFORM isro:startLog("PERFORM", "isro:importExternalJar");
		
		$object = $loader.loadJar($path, $classFullName);	
		$className = $loader.getClassName($classFullName);
		
		$handler.assertObject($className, $object);	
		
		PERFORM isro:endLog("PERFORM", "isro:importExternalJar");
}
