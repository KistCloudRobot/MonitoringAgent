PLAN PERFORM UpdateTaskProgress($robotID, $taskName) {
	ID: "http://www.arbi.com//UpdateTaskProgress"
	PRECONDITION :
		FACT TaskManager($tm); 
		FACT AgentTaskStatus($robotID, $id, $taskName, $status);
	BODY:
		PERFORM isro:startLog("PERFORM", "UpdateTaskProgress");

		$progress = $tm.getTaskProgress($robotID, $taskName);
	
		$data = "(TaskProgress \"" + $robotID + "\" \"" + $progress + "\")";
		PERFORM isro:argumentLog("$TaskProgress", $data);
		$tm.assertFact($data);
		
		PERFORM isro:endLog("PERFORM", "UpdateTaskProgress");

	UTILITY: 10000; 
}